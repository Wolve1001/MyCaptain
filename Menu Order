#include<stdio.h>
int main()
{
	int c;
	printf("Please select any number between 1-5.\n");
	scanf("%d",&c);
	switch(c)
	{
		case 1:
			printf("Food item - Pasta\n");
			printf("Price - Rs.239");
			break;
		case 2:
		    printf("Food item - Burger\n");
		    printf("Price - Rs.129");
		    break;
		case 3:	
		    printf("Food item - Pasta\n");
		    printf("Price - Rs.179");
		    break;
		case 4:
		    printf("Food item - French Fries\n");
			printf("Price - Rs.99");
			break;
		case 5:
		    printf("Food item - Sandwich\n");
			printf("Price - 149");
			break;
		default:
			printf("Invalid choice.");	    
	}


	return 0;
}
